{
    "name": "gateio/gateapi-php",
    "version": "6.100.2",
    "description": "Welcome to Gate API  APIv4 provides spot, margin and futures trading operations. There are public APIs to retrieve the real-time market statistics, and private APIs which needs authentication to trade on user's behalf.",
    "keywords": [
        "gate",
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://www.gate.io",
    "license": "unlicense",
    "authors": [
        {
            "name": "Gate",
            "homepage": "https://www.gate.io"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~3.6",
        "friendsofphp/php-cs-fixer": "~2.17"
    },
    "autoload": {
        "psr-4": { "GateApi\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "GateApi\\" : "test/" }
    }
}
